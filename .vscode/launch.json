    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Terminal (integrated)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "args": [
                "-jid=debug",
                "-t",
                "-em=wiki-50",
                "-fe",
                "-ds=debug2"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "GC-Machine-Type-Arg-Test",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/submit_job.py",
            "args": [
                "--job-id=\"testing_bucket_param\"",
                "--job-dir=\"debug-data\"",
                "--staging-bucket=\"tsaplay-bucket-test\"",
                "--job-labels", 
                "type=example",
                // "--machine-types=\"masterType=complex_model_s workerType=complex_model_s\"", 
                "--task-args",
                "batch",
                "batch_tasks.txt"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Submit Batch Job to GCP with Comet API",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/submit_job.py",
            "args": [
                "-jid=debuging",
                "-t",
                "batch",
                "reproducibility-test.txt",
                "--defaults",
                "-cmt",
                "T1NrVnn32dXWeOxeQWGArkHwc",
                "-wrk",
                "msc-reproducibility",
                "-aux",
                "logging=true",
                "oov_fn='uniform[-0.1,0.1]'"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "batch-defaults-arg-test",
            "type": "python",
            "request": "launch",
            "module": "tsaplay.task",
            "args": [
                "batch",
                "reproducibility-test.txt",
                "--defaults",
                "-b",
                "5",
                "--job-dir",
                "ajobdir"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Epoch Argument Testing",
            "type": "python",
            "request": "launch",
            "module": "tsaplay.task",
            "args": [
                "single",
                "-em='twitter-100[corpus]'",
                "-ds",
                "dong",
                "-m",
                "tclstm",
                "-b",
                "100",
                "-e",
                "5",
                "-mp",
                "oov_train=1",
                "-cmt",
                "T1NrVnn32dXWeOxeQWGArkHwc",
                "-contd",
                "testing-epoch-argument"
                // "oov_fn='uniform[-0.1,0.1]'",
                // "hidden_units=5",
                // "-aux",
                // "attn_heatmaps=false",
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Dataset-redist-param-test",
            "type": "python",
            "request": "launch",
            "module": "tsaplay.task",
            "args": [
                "single",
                "-em='twitter-50[corpus]'",
                "-ds", 
                "'dong[33/34/33,15/70/15]'",
                "nakov",
                "-m=lstm",
                "-b=25",
                "-s=200",
                "-mp",
                "num_oov_buckets=100"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Current File (Integrated Terminal)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Attach",
            "type": "python",
            "request": "attach",
            "port": 5678,
            "host": "localhost"
        },
        {
            "name": "Python: Module",
            "type": "python",
            "request": "launch",
            "module": "enter-your-module-name-here",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Django",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "console": "integratedTerminal",
            "args": [
                "runserver",
                "--noreload",
                "--nothreading"
            ],
            "django": true
        },
        {
            "name": "Python: Flask",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "app.py"
            },
            "args": [
                "run",
                "--no-debugger",
                "--no-reload"
            ],
            "jinja": true
        },
        {
            "name": "Python: Current File (External Terminal)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "externalTerminal"
        }
    ]
}