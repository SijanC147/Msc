    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Terminal (integrated)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "args": [
                "-jid=debug",
                "-t",
                "-em=wiki-50",
                "-fe",
                "-ds=debug2"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "GC-Machine-Type-Arg-Test",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/submit_job.py",
            "args": [
                "--job-id=\"testing_bucket_param\"",
                "--job-dir=\"debug-data\"",
                "--staging-bucket=\"tsaplay-bucket-test\"",
                "--job-labels", 
                "type=example",
                // "--machine-types=\"masterType=complex_model_s workerType=complex_model_s\"", 
                "--task-args",
                "batch",
                "batch_tasks.txt"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "GC-Job-Sumission-Test",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/submit_job.py",
            "args": [
                "-jid=debug",
                "-t",
                "batch",
                "batch_tasks.txt"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Embedding-filter-fn-test",
            "type": "python",
            "request": "launch",
            "module": "tsaplay.task",
            "args": [
                "single",
                "-em='twitter-50[corpus,only_adjectives]'",
                "-ds='dong'",
                "-m=lstm",
                "-b=25",
                "-s=200",
                "-mp",
                "num_oov_buckets=100"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Dataset-redist-param-test",
            "type": "python",
            "request": "launch",
            "module": "tsaplay.task",
            "args": [
                "single",
                "-em='twitter-50[corpus]'",
                "-ds", 
                "'dong[33/34/33,15/70/15]'",
                "nakov",
                "-m=lstm",
                "-b=25",
                "-s=200",
                "-mp",
                "num_oov_buckets=100"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Current File (Integrated Terminal)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Attach",
            "type": "python",
            "request": "attach",
            "port": 5678,
            "host": "localhost"
        },
        {
            "name": "Python: Module",
            "type": "python",
            "request": "launch",
            "module": "enter-your-module-name-here",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Django",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "console": "integratedTerminal",
            "args": [
                "runserver",
                "--noreload",
                "--nothreading"
            ],
            "django": true
        },
        {
            "name": "Python: Flask",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "app.py"
            },
            "args": [
                "run",
                "--no-debugger",
                "--no-reload"
            ],
            "jinja": true
        },
        {
            "name": "Python: Current File (External Terminal)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "externalTerminal"
        }
    ]
}